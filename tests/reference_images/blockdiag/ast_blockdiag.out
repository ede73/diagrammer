blockdiag {
    default_fontsize = 14
    orientation=portrait
    group "GraphObject"{
        
        label="GraphObject"
        name[ color="#ccccff", shape=box ];
        color[ color="#ccccff", shape=box ];
        textColor[ color="#ccccff", shape=box ];
        label[ color="#ccccff", label="label\nspecial syntax with :: for links\n(label::xlabel)", shape=box ];
    }
    group "GraphRoot:GraphObject"{
        
        label="GraphRoot:GraphObject"
        OBJECTS1[ label="OBJECTS", shape=flowchart.database ];
        xGenerator[ color="#ccffcc", label="generator", shape=box ];
        xVisualizer[ color="#ccffcc", label="visualizer", shape=box ];
        direction[ color="#ccffcc", shape=box ];
        xStart[ color="#ccffcc", label="start", shape=box ];
        xEqual[ color="#ccffcc", label="equal", shape=box ];
        currentShape[ shape=box ];
        default2[ label="default\nnodecolor\ngroupcolor\nlinkcolor", shape=box ];
    }
    group "Group:GraphObject"{
        
        label="Group:GraphObject"
        equal2[ label="equal", shape=box ];
        OBJECTS2[ label="OBJECTS", shape=flowchart.database ];
    }
    group "Node:GraphObject"{
        
        label="Node:GraphObject"
        xShape[ label="shape", shape=box ];
        image[ shape=box ];
        style[ shape=box ];
    }
    group "Link:GraphObject"{
        
        label="Link:GraphObject"
        linkType[ shape=box ];
        left[ shape=box ];
        right[ shape=box ];
    }
    GraphObject -> GraphRoot[ color="#0000ff", label="extends(GraphObject->GraphRoot)", style="dotted" ];
    GraphObject -> Node[ color="#0000ff", label="extends(GraphObject->Node)", style="dotted" ];
    GraphObject -> xGroup[ color="#0000ff", label="extends(GraphObject->xGroup)", style="dotted" ];
    GraphObject -> Link[ color="#0000ff", label="extends::extends(GraphObject->Link)", style="dotted" ];
    left -> Node[ color="#00ff00", label="links", style="dashed" ];
    left -> xGroup[ color="#00ff00", label="links", style="dashed" ];
    right -> xGroup[ color="#00ff00", label="links", style="dashed" ];
    right -> Node[ color="#00ff00", label="links", style="dashed" ];
    OBJECTS1 -> Node[ color="#ff0000", label="contains" ];
    OBJECTS1 -> xGroup[ color="#ff0000", label="contains" ];
    OBJECTS2 -> Node[ color="#ff0000", label="contains" ];
    OBJECTS2 -> xGroup[ color="#ff0000", label="contains" ];
}
