digraph {
    compound=true;
    rankdir=TD;
    xstart[ label="start", shape="diamond" ];
    end[ shape="square" ];
    subgraph cluster_1 {
        label="process222";
        a0[ shape="ellipse", style="rounded" ];
        a1[ shape="ellipse" ];
        a2[ shape="ellipse" ];
        a3[ shape="ellipse", style="solid" ];
    }//end of 1 undefined
    subgraph cluster_2 {
        label="process 2";
        b0[ shape="ellipse" ];
        b1[ shape="ellipse" ];
        b2[ shape="ellipse", style="dashed" ];
        b3[ shape="ellipse", style="invis" ];
    }//end of 2 undefined
    //links start
    a0->a1[ style="dotted" ];
    a1->a2[ style="dashed" ];
    a2->a3[ dir=both ];
    b0->b1[ color="#ff0000" ];
    b1->b2;
    b2->b3[ color="#00ff00", fontcolor="#ff0000", label="b2 b3 link" ];
    xstart->a0;
    a0->b0;
    a1->b3[ label="a1 b2 link comment" ];
    b2->a3;
    a3->a0;
    a3->end;
    b3->end;
