blockdiag{
 default_fontsize = 14
  orientation=landscape
  xstart[shape="minidiamond"];
  end[shape="box"];
  group "process #1"{
    color="#ff0000" 
    label="process #1" 
    a0[shape=ellipse];
    a1[shape=ellipse];
    a2[shape=ellipse];
    a3[shape=ellipse];
  }
  group "process #2"{
    color="#00ff00" 
    label="process #2" 
    b0[shape=ellipse];
    b1[shape=ellipse];
    b2[shape=ellipse];
    b3[shape=ellipse];
  }
  a0 -> a1;
  a1 -> a2;
  a2 -> a3;
  b0 -> b1;
  b1 -> b2;
  b2 -> b3;
  xstart -> a0;
  xstart -> b0;
  a1 -> b3;
  b2 -> a3;
  a3 -> a0;
  a3 -> end;
  b3 -> end;
}
