    database [ shape="cylinder" 		width=1   fixedsize=true color=blue style=filled fontcolor=yellow];
    default [ shape="box" 			width=1   fixedsize=true color=blue style=filled fontcolor=yellow];
    diamond [ shape="diamond" 			width=1   fixedsize=true color=blue style=filled fontcolor=yellow];
    dots [ shape="point" 			width=1   fixedsize=true color=blue style=filled fontcolor=yellow];
    doublecircle [ shape="doublecircle" 	width=1   fixedsize=true color=blue style=filled fontcolor=yellow];
