digraph {
    compound=true;
    rankdir=TD;
    ablack;
    b[label="label"];
    cblue[fillcolor="#0000ff",style="filled"];
    dred[fillcolor="#ff0000",style="filled",label="red with label, c with blue, b has label, a without, e links to f"];
    eblack;
    fblack;
    gblack;
    hblack;
    iblue[fillcolor="#0000ff",style="filled"];
    j;
    klightblue[fillcolor="#00ffff",style="filled"];
    l;
    myellow[fillcolor="#ffff00",style="filled"];
    n;
    ogray[fillcolor="#777777",style="filled"];
    p;
    qred[fillcolor="#ff0000",style="filled"];
    rblue[fillcolor="#0000ff",style="filled"];
    //links start
    eblack->fblack;
    gblack->hblack[label="g links to h"];
    iblue->j;
    klightblue->l[label="k links to l"];
    myellow->n[color="#ff00ff",fontcolor="#ff00ff"];
    ogray->p[label="o p link orange",color="#ff7777",fontcolor="#ff7777"];
    qred->rblue[label="q r link green",color="#00ff00",fontcolor="#00ff00"];
