blockdiag{
 default_fontsize = 14
  orientation=landscape
  outside1[color="#ff0000"  ,label="Group to node"];
  outside3[color="#ff0000"  ,label="Inner group to node"];
  group "undefined"{


    ma[color="#ff0000"];
    innermaster1[color="#999999"];
    innermaster2[color="#ffaaaa"];
  }
  outside2[color="#ffff00"  ,label="Node to group"];
  outside4[color="#ffff00"  ,label="Node to inner group"];
  group "undefined"{


    o[color="#ffff00"];
  }
  ma -> ima1;
  ma -> ima2;
  ima2 -> o;
  outside2 -> master;
  master -> outside1;
  innermaster1 -> outside3;
  outside4 -> innermaster2;
}
