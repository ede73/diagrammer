digraph {
  compound=true;
  rankdir=TD;
  subgraph cluster_joku {
        label="label tassa"; 
    style=filled;
        color="#ff0000";
 
      joku1;
      joku2;
  }//end of joku
  subgraph cluster_toinen {
        label="label tassa"; 
    style=filled;
        color="#00ff00";
 
      toinen1;
      toinen2;
  }//end of toinen
//links start
  joku1->toinen1[label="node to node"];
  joku2->toinen1[label="node to group", lhead=cluster_toinen];
}
