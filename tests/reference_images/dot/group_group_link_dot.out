digraph {
    compound=true;
    rankdir=TD;
    subgraph cluster_z {
        label="Group1";
        x;
    }//end of z undefined
    subgraph cluster_a {
        label="Group2";
        b;
    }//end of a undefined
    subgraph cluster_c {
        label="Group3";
        d;
    }//end of c undefined
    //links start
    x->b[  lhead=cluster_a,  ltail=cluster_z, label="edge1" ];
    x->d[  lhead=cluster_c,  ltail=cluster_z, label="edge1" ];
}
