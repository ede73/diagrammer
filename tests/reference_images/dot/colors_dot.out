digraph {
    compound=true;
    rankdir=TD;
    explanation[ fillcolor="#ffff00", fontcolor="#ff00ff", label="Default colors are:\nRed edges\nGreen edge texts\nGroups are blue(filled)\nVertices are filled yellow, but having pink texs", shape="box", style="filled" ];
    default_yellow1[ fillcolor="#ffff00", fontcolor="#ff00ff", style="filled" ];
    default_yellow2[ fillcolor="#ffff00", fontcolor="#ff00ff", style="filled" ];
    not_default_black[ fillcolor="#000000", fontcolor="#ff00ff", style="filled" ];
    not_default_green[ fillcolor="#00ff00", fontcolor="#ff00ff", style="filled" ];
    amarant[ fillcolor="#9F2B68", fontcolor="#ff00ff", label="Amaranth colored node from variable", style="filled" ];
    not_default_darksalmon[ fillcolor="#E9967A", fontcolor="#ff00ff", style="filled" ];
    not_default_peachpuff[ fillcolor="#FFDAB9", fontcolor="#ff00ff", style="filled" ];
    subgraph cluster_agroup {
        label="Here's a default colored group,ie.blue";
        style=filled;
        color="#0000ff";
        group_default_yellow1[ fillcolor="#ffff00", fontcolor="#ff00ff", style="filled" ];
        group_default_yellow[ fillcolor="#ffff00", fontcolor="#ff00ff", style="filled" ];
        group_not_default_black[ fillcolor="#000000", fontcolor="#ff00ff", style="filled" ];
        group_not_default_green[ fillcolor="#00ff00", fontcolor="#ff00ff", style="filled" ];
        group_not_default_darksalmon[ fillcolor="#E9967A", fontcolor="#ff00ff", style="filled" ];
        group_not_default_peachpuff[ fillcolor="#FFDAB9", fontcolor="#ff00ff", style="filled" ];
    }//end of agroup undefined
    subgraph cluster_anondefaultgroup {
        label="Here's a group with new context defaults";
        style=filled;
        color="#0000ff";
        group_default_amaranth[ fillcolor="#9F2B68", fontcolor="#2E8B57", style="filled" ];
        group_not_default_black2[ fillcolor="#000000", fontcolor="#2E8B57", style="filled" ];
        group_not_default_green2[ fillcolor="#00ff00", fontcolor="#2E8B57", style="filled" ];
        subgraph cluster_reddefaultgrp {
            label="New default red group";
            style=filled;
            color="#ff0000";
            xxx[ fillcolor="#9F2B68", fontcolor="#2E8B57", label="Amaranth(#9F2B68) colored node", style="filled" ];
        }//end of reddefaultgrp undefined
    }//end of anondefaultgroup undefined
    //links start
    default_yellow1->default_yellow2[ color="#ff0000", fontcolor="#00ff00", label="red edge, green text" ];
    not_default_black->not_default_green[ color="#ffff00", fontcolor="#ff0000", label="Edge is yellow(#ffff00),\nbut edge color is red(#ff0000)\n(overriding defaults)" ];
    not_default_darksalmon->not_default_peachpuff[ color="#2E8B57", fontcolor="#9400D3", label="Edge is seagreen(#2E8B57),\nbut edge color is darkviolet(#9400D3)\n(overriding defaults)" ];
    group_default_yellow1->group_default_yellow[ color="#ff0000", fontcolor="#00ff00", label="red edge, green text" ];
    group_not_default_black->group_not_default_green[ color="#ffff00", fontcolor="#ff0000", label="Edge is yellow(#ffff00),\nbut edge color is red(#ff0000)\n(overriding defaults)" ];
    group_not_default_darksalmon->group_not_default_peachpuff[ color="#2E8B57", fontcolor="#9400D3", label="Edge is seagreen(#2E8B57),\nbut edge color is darkviolet($(darkvioleta))\n(overriding defaults)" ];
    group_default_amaranth->group_default_amaranth[ color="#E9967A", fontcolor="#FFDAB9", label="darksalmon(#E9967A) edge, peachpuff(#FFDAB9) text" ];
    group_not_default_black2->group_not_default_green2[ color="#ffff00", fontcolor="#ff0000", label="Edge is yellow(#ffff00),\nbut edge color is red(#ff0000)\n(overriding defaults)" ];
}
